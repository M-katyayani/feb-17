n=int(input('enter n value'))
sum1=0
sum2=0
for i in range(1,1+n):
    if i%2==1:
        sum1=sum1+i
        print('sum1:',sum1)
        
    elif i%2==0:
        sum2=sum2+i
        print('sum2:',sum2)
        
        OUTPUT
        
       enter n value 5
       sum1:1
       sum2:2
       sum1:4
       sum2:6
